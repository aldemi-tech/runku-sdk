{
  "name": "@runku/sdk",
  "version": "0.1.0",
  "description": "Runku SDK for JavaScript/TypeScript applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "eventemitter3": "5.0.1"
  },
  "devDependencies": {
    "tsup": "8.3.5",
    "typescript": "5.7.3"
  },
  "peerDependencies": {},
  "files": [
    "dist"
  ],
  "keywords": [
    "runku",
    "baas",
    "backend",
    "sdk",
    "database",
    "realtime"
  ],
  "license": "MIT"
}
